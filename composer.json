{
	"name" : "motana/multikernel",
	"version" : "1.3.5",
	"type" : "symfony-bundle",
	"description" : "Motana Multi-Kernel Bundle",
	"keywords" : [
		"symfony",
		"kernel",
		"multi-kernel"
	],
	"homepage" : "http://github.com/KintradimCrux/motana-multikernel/",
	"license" : "MIT",
	"authors" : [{
			"name" : "Wenzel Jonas",
			"email" : "mail@ramihyn.sytes.net",
			"role" : "Developer"
		}
	],
	"require" : {
		"php" : ">=5.6",
		"symfony/symfony" : "3.3.*",
		"symfony/monolog-bundle" : "^3.0.2",
		"sensio/generator-bundle" : "^3.1",
		"sensio/framework-extra-bundle" : "^3.0.2",
		"symfony/phpunit-bridge": "^4.0"
	},
	"require-dev" : {
		"doctrine/orm" : "^2.5",
		"doctrine/doctrine-bundle" : "^1.6",
		"doctrine/doctrine-cache-bundle" : "^1.3",
		"symfony/swiftmailer-bundle" : "^2.0",
		"sensio/distribution-bundle" : "^5.0",
		"satooshi/php-coveralls" : "^2.0"
	},
	"autoload" : {
		"psr-4" : {
			"Motana\\Bundle\\MultikernelBundle\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Motana\\Bundle\\MultikernelBundle\\Tests\\" : "tests/"
		}
	}
}
