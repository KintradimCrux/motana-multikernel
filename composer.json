{
	"name" : "motana/multikernel",
	"version" : "1.2.3",
	"type" : "symfony-bundle",
	"description" : "Motana Multi-Kernel Bundle",
	"keywords" : [
		"symfony",
		"kernel",
		"multi-kernel"
	],
	"homepage" : "http://github.com/KintradimCrux/motana-multikernel/",
	"license" : "MIT",
	"authors" : [{
			"name" : "Wenzel Jonas",
			"email" : "mail@ramihyn.sytes.net",
			"role" : "Developer"
		}
	],
	"require" : {
		"php" : ">=5.6",
		"symfony/symfony" : "^3.2",
		"symfony/monolog-bundle" : "^3.0.2"
	},
	"require-dev" : {
		"phpunit/phpunit" : "^5.7",
		"satooshi/php-coveralls" : "dev-master"
	},
	"autoload" : {
		"psr-4" : {
			"Motana\\Bundle\\MultikernelBundle\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Tests\\Motana\\Bundle\\MultikernelBundle\\" : "tests/"
		}
	}
}