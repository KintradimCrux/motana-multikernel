            "name": "debug:config",
            "usage": [
                "bin\/console app debug:config [<name>] [<path>]"
            ],
            "description": "Dumps the current configuration for an extension",
            "help": "The <info>debug:config<\/info> command dumps the current configuration for an\nextension\/bundle.\n\nEither the extension alias or bundle name can be used:\n\n  <info>bin\/console debug:config framework<\/info>\n  <info>bin\/console debug:config FrameworkBundle<\/info>\n\nFor dumping a specific option, add its path as second argument:\n\n  <info>bin\/console debug:config framework serializer.enabled<\/info>",
            "definition": {
                "arguments": {
                    "name": {
                        "name": "name",
                        "is_required": false,
                        "is_array": false,
                        "description": "The bundle name or the extension alias",
                        "default": null
                    },
                    "path": {
                        "name": "path",
                        "is_required": false,
                        "is_array": false,
                        "description": "The configuration option path",
                        "default": null
